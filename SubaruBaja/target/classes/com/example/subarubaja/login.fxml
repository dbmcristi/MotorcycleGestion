<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="179.0" prefWidth="227.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.subarubaja.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <AnchorPane prefHeight="391.0" prefWidth="594.0">
         <children>
            <Button fx:id="loginButton" layoutX="169.0" layoutY="51.0" mnemonicParsing="false" onAction="#checkIfIsLoggedNewStage" text="Login" />
            <TextField fx:id="userNameNewStage" layoutX="14.0" layoutY="39.0" promptText="username" />
            <TextField fx:id="passwordNewStage" layoutX="14.0" layoutY="77.0" promptText="password" />
         </children></AnchorPane>
   </children>
</VBox>
